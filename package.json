{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && npm run lint && vite build",
    "preview": "vite preview",
    "lint": "eslint \"src/**/*.{vue,ts,js}\" --cache",
    "lint:fix": "eslint \"src/**/*.{vue,ts,js}\" --fix --cache",
    "prepare": "husky"
  },
  "dependencies": {
    "axios": "^1.9.0",
    "element-plus": "^2.9.10",
    "nprogress": "^0.2.0",
    "pinia": "^3.0.2",
    "vue": "^3.5.13",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.8.1",
    "@commitlint/config-conventional": "^17.8.1",
    "@eslint/css": "^0.7.0",
    "@eslint/js": "^9.26.0",
    "@eslint/json": "^0.12.0",
    "@eslint/markdown": "^6.4.0",
    "@types/nprogress": "^0.2.3",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.26.0",
    "eslint-plugin-vue": "^10.1.0",
    "globals": "^16.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.2",
    "prettier": "^3.5.3",
    "sass": "^1.89.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.32.0",
    "vite": "^6.3.5",
    "vite-plugin-checker": "^0.9.3",
    "vue-tsc": "^2.2.8"
  },
  "lint-staged": {
    "*.{vue,ts,js}": [
      "npm run lint:fix -- --max-warnings 0"
    ],
    "*.{json,md,yml}": [
      "prettier --write"
    ]
  }
}
